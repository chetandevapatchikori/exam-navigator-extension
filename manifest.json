
{
    "manifest_version": 2,
    "name": "exam navigator",
    "description": "preventing cheating during exams",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },

    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]

        }
    ],

    "background":{
        "scripts": ["background.js"]
    },

    "browser_action":{
        "default_icon": "cn.png"
    }
}